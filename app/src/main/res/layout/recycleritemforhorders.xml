<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardElevation="5dp"
        app:cardCornerRadius="8dp"
        app:cardUseCompatPadding="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/textview_bookingroomname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="hona"
                android:textSize="18dp"
                android:padding="5dp"></TextView>
            <TextView
                android:id="@+id/textview_bookingusername"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="F.I.SH"
                android:padding="5dp"
                android:textSize="18dp"></TextView>
            <TextView
                android:id="@+id/textview_bookingusernumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="phonr number"
                android:padding="5dp"
                android:textSize="18dp"></TextView>
            <TextView
                android:id="@+id/textview_bookingdatatime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="datatime"
                android:padding="5dp"
                android:textSize="15dp"></TextView>
        </LinearLayout>

    </androidx.cardview.widget.CardView>


</RelativeLayout>